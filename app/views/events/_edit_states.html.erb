<% cntry = params[:cntry_name] if cntry.nil? %>
<dt>
  <%= fedora_field_label("properties", [:state], "State:") %>
</dt>
<%- if(!cntry.nil? && cntry.eql?("United States")) -%>
  <dd>
    <%= fedora_select(@document_fedora,"properties", [:state], :choices=>StateList.states("US")) %>
  </dd>
<%- else -%>
  <dd class="state_text_field">
    <%= fedora_text_field(@document_fedora,"properties", [:state]) %>
  </dd>
<%- end -%>
