- if result["active_fedora_model_s"].is_a?(Array) && result["active_fedora_model_s"].include?("RedirectedAsset")
  %tr.file_asset{:id=>result["id"]}
    %td
      =link_to result[solr_name("title", :text)], redirected_asset_path(result["id"])
    %td
      =result[solr_name("extent", :text)]
    %td
      =format_date(DateTime.parse(result[solr_name("system_create", :date)]))
    %td
      - if editor?
        - if @container.nil?
          - file_asset_url = file_asset_path(result["id"])
        - else
          - file_asset_url = asset_file_asset_path(@container.pid, result["id"])      
        %a.destroy_file_asset{:href=>file_asset_url, :title=>"Remove file"} Delete
- else
  %tr.file_asset{:id=>result["id"]}
    %td
      =link_to result[solr_name("title", :text)], file_asset_path(result["id"])
    %td
      =result[solr_name("extent", :text)]
    %td
      =format_date(DateTime.parse(result[solr_name("system_create", :date)]))
    %td
      - if editor?
        - if @container.nil?
          - file_asset_url = file_asset_path(result["id"])
        - else
          - file_asset_url = asset_file_asset_path(@container.pid, result["id"])        
        %a.destroy_file_asset{:href=>file_asset_url, :title=>"Remove file"} Delete
