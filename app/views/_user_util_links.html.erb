<% if current_user %>

    <ul>
      <li onclick="javascript:createAssetNavigateTo(this, '<%= url_for(:action => "new", :controller => "events", :content_type => "event") %>');">Add an Event</li>
      <li><%= current_user.login %> (<%= link_to "Log Out", logout_path %>)</li>
    </ul>

<% else %>
    <li><%= link_to 'Login', login_path %></li>
<% end %>
