<div id="links">
<%- unless @redirected_objects.nil? || @redirected_objects.empty? -%>
  <%# if params[:deletable] == "true" -%>
    <%#<ul>%>
      <%#render :partial=>'deletable_result', :collection=>@redirected_objects, :container_id=>params[:id]%>
    <%#</ul>%>
  <%# else -%>
    <table id="redirected_assets" class="multipleEdit">
      <%=render :partial=>'redirected_assets/result', :collection=>@redirected_objects, :container_id=>params[:id] %>
    </table>
  <%# end -%>
<%- else -%>
  <h3><i>No current links</i></h3>  
<%- end -%>
</div>
