<%- unless @redirected_objects.nil? || @redirected_objects.hits.empty? -%>
  <%# if params[:deletable] == "true" -%>
    <%#<ul>%>
      <%#render :partial=>'deletable_result', :collection=>@redirected_objects.hits %>
    <%#</ul>%>
  <%# else -%>
    <table id="redirected_assets" class="multipleEdit">
      <%=render :partial=>'redirected_assets/result', :collection=>@redirected_objects.hits %>
    </table>
  <%# end -%>
<%- else -%>
  <h3><i>No current links</i></h3>  
<%- end -%>
